<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    helpPath="en/cq/current/wcm/default_components.html#Title"
    title="React Content"
    xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <react-template
            jcr:primaryType="cq:Widget"
            defaultValue="/apps/react-demo-3/components/sign-in/react-templates/default.jsx"
            fieldLabel="Template"
            name="./jcr:reactTemplate"
            type="select"
            xtype="selection">
            <options jcr:primaryType="cq:WidgetCollection">
                <default
                    jcr:primaryType="nt:unstructured"
                    text="default"
                    value="/apps/react-demo-3/components/sign-in/react-templates/default.jsx"/>
                <alternate
                    jcr:primaryType="nt:unstructured"
                    text="alternate"
                    value="/apps/react-demo-3/components/sign-in/react-templates/alternate.jsx"/>
            </options>
        </react-template>
        <react-content
            jcr:primaryType="cq:Widget"
            fieldDescription=""
            fieldLabel="Content"
            name="./jcr:reactContent"
            rows="15"
            xtype="richtext"/>
    </items>
</jcr:root>
